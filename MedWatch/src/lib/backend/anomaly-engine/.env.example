# Server Configuration
PORT=3002
NODE_ENV=development

# Logging
LOG_LEVEL=info

# Alert Configuration
ALERT_EMAIL_SERVICE=smtp
ALERT_EMAIL_HOST=smtp.example.com
ALERT_EMAIL_PORT=587
ALERT_EMAIL_USER=your-email@example.com
ALERT_EMAIL_PASS=your-password
ALERT_EMAIL_FROM=alerts@medwatch.com

# Processing Configuration
BATCH_SIZE=1000
PROCESSING_INTERVAL=30000
ALERT_THRESHOLD=0.7

# ML Model Configuration
ENABLE_ML_MODELS=true
MODEL_UPDATE_INTERVAL=86400000
